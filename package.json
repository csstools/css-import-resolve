{
  "name": "css-import-resolve",
  "version": "1.0.0",
  "description": "An algorithm for resolving imports in CSS",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "csstools/css-import-resolve",
  "homepage": "https://github.com/csstools/css-import-resolve#readme",
  "bugs": "https://github.com/csstools/css-import-resolve/issues",
  "main": "resolve.dist.js",
  "module": "resolve.js",
  "files": [
    "resolve.js",
    "resolve.dist.js"
  ],
  "scripts": {
    "start": "babel resolve.js --presets=env --plugins=add-module-exports --out-file resolve.dist.js",
    "test": "npm start && npm run test:lint && npm run test:tape",
    "test:lint": "eslint resolve.js --cache --ignore-pattern .gitignore",
    "test:tape": "node .tape"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.12.1",
    "eslint-config-dev": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "rules": {
      "no-param-reassign": 0
    }
  },
  "keywords": [
    "css",
    "spec",
    "imports",
    "resolve",
    "algorithm",
    "external",
    "files",
    "style",
    "sheets"
  ]
}
